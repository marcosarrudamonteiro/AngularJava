<mat-toolbar color="accent" > <!--'primary', 'accent', or 'warn' -->
  <span>Sistema de Controle de Servi√ßos</span>
  <div>
    <button mat-icon-button matTooltip="Application Menu" (click)="sidenav.toggle()">
      <mat-icon>chevron_right</mat-icon>
    </button>
  </div>
  <div>
  <button mat-icon-button matTooltip="Pesquisar">
    <mat-icon>search</mat-icon>
  </button>
  <button mat-icon-button matTooltip="Notifications">
    <mat-icon>notifications</mat-icon>
  </button>
  <button mat-icon-button matTooltip="My Account">
    <mat-icon>account_circle</mat-icon>
  </button>
  <mat-menu #accountMenu yPosition="below">
    <button mat-menu-item routerLink="myaccount">
      <mat-icon>person</mat-icon>
      <span>My Account</span>
    </button>
    <button mat-menu-item>
      <mat-icon>help</mat-icon>
      <span>Help</span>
    </button>
    <mat-divider></mat-divider>
    <button mat-menu-item>
      <mat-icon>exit_to_app</mat-icon>
      Logout
    </button>
  </mat-menu>
  </div>
</mat-toolbar>
